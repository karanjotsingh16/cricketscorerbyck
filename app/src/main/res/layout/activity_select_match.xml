<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".selectMatch">

    <ImageView
        android:id="@+id/select_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/selectmatch" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="118dp"
        android:layout_height="92dp"
        android:layout_marginTop="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/select_txt"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/test"
        android:layout_width="151dp"
        android:layout_height="152dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="-30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/select_txt"
        app:srcCompat="@drawable/test" />

    <ImageView
        android:id="@+id/odi"
        android:layout_width="153dp"
        android:layout_height="152dp"
        android:layout_marginTop="-30dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/select_txt"
        app:srcCompat="@drawable/odi" />

    <ImageView
        android:id="@+id/test_txt"
        android:layout_width="68dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="@+id/test"
        app:layout_constraintStart_toStartOf="@+id/test"
        app:layout_constraintTop_toBottomOf="@+id/test"
        app:srcCompat="@drawable/test_text" />

    <ImageView
        android:id="@+id/odi_txt"
        android:layout_width="113dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="@+id/odi"
        app:layout_constraintStart_toStartOf="@+id/odi"
        app:layout_constraintTop_toBottomOf="@+id/odi"
        app:srcCompat="@drawable/odi_text" />

    <ImageView
        android:id="@+id/t20"
        android:layout_width="151dp"
        android:layout_height="152dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="@+id/test"
        app:layout_constraintTop_toBottomOf="@+id/test_txt"
        app:srcCompat="@drawable/t20" />

    <ImageView
        android:id="@+id/custom_img"
        android:layout_width="151dp"
        android:layout_height="152dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="@+id/odi"
        app:layout_constraintTop_toBottomOf="@+id/odi_txt"
        app:srcCompat="@drawable/custom" />

    <ImageView
        android:id="@+id/t20_txt"
        android:layout_width="59dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="@+id/t20"
        app:layout_constraintStart_toStartOf="@+id/t20"
        app:layout_constraintTop_toBottomOf="@+id/t20"
        app:srcCompat="@drawable/t20_text" />

    <ImageView
        android:id="@+id/custom_txt"
        android:layout_width="119dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="@+id/custom_img"
        app:layout_constraintStart_toStartOf="@+id/custom_img"
        app:layout_constraintTop_toBottomOf="@+id/custom_img"
        app:srcCompat="@drawable/custom_text" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Â©Karan Creations. All Rights Reserved."
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t20_txt" />

